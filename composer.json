{
    "name": "itised/mirelap",
    "type": "library",
    "description": "REST API package for Laravel",
    "authors": [
        {
            "name": "Edward DeMaio",
            "email": "ed@itsed.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "laravel/framework": "5.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "illuminate/console": "5.2.*",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Mirelap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mirelap\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
